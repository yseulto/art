// \uc0\u36628 \u21161 \u20989 \u24335 \u65306 \u23559  Google Drive View \u36899 \u32080 \u36681 \u25563 \u28858 \u26356 \u31337 \u23450 \u30340  Thumbnail \u30452 \u25509 \u36899 \u32080 \
const getDriveDirectLink = (id) => \{\
  return `https://drive.google.com/thumbnail?id=$\{id\}&sz=w1920`;\
\};\
