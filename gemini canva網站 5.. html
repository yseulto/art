export default function App() \{\
  const [activeTab, setActiveTab] = useState('home'); \
  const [mainNav, setMainNav] = useState('home'); \
  \
  // Lightbox \uc0\u29376 \u24907 \u31649 \u29702 \
  const [lightboxOpen, setLightboxOpen] = useState(false);\
  const [selectedAlbum, setSelectedAlbum] = useState(null);\
\
  // \uc0\u20840 \u31449 \u23433 \u20840 \u20445 \u35703 \u65306 \u31105 \u27490 \u21491 \u37749 \u12289 \u35079 \u35069 \u12289 \u25302 \u26355 \
  useEffect(() => \{\
    // 1. \uc0\u31105 \u27490 \u21491 \u37749  (Context Menu)\
    const handleContextMenu = (e) => \{\
      e.preventDefault();\
    \};\
\
    // 2. \uc0\u31105 \u27490 \u25302 \u26355 \u22294 \u29255  (Drag Start)\
    const handleDragStart = (e) => \{\
      if (e.target.tagName === 'IMG') \{\
        e.preventDefault();\
      \}\
    \};\
    \
    // 3. \uc0\u31105 \u27490 \u37749 \u30436 \u35079 \u35069 \u25463 \u24465  (Ctrl+C, Cmd+C)\
    const handleKeyDown = (e) => \{\
      // \uc0\u25876 \u25130  Ctrl+C \u25110  Cmd+C\
      if ((e.ctrlKey || e.metaKey) && (e.key === 'c' || e.key === 'C')) \{\
        e.preventDefault();\
      \}\
      // \uc0\u25876 \u25130  Ctrl+A (\u20840 \u36984 ) - \u36984 \u25799 \u24615 \u21152 \u20837 \
      if ((e.ctrlKey || e.metaKey) && (e.key === 'a' || e.key === 'A')) \{\
        e.preventDefault();\
      \}\
      // \uc0\u25876 \u25130  F12 \u25110  Ctrl+Shift+I (\u38283 \u30332 \u32773 \u24037 \u20855 ) - \u21487 \u36984 \u65292 \u20294 \u36890 \u24120 \u28961 \u27861 \u23436 \u20840 \u38450 \u22581 \
      // if (e.key === 'F12' || (e.ctrlKey && e.shiftKey && e.key === 'I')) \{\
      //   e.preventDefault();\
      // \}\
    \};\
\
    document.addEventListener('contextmenu', handleContextMenu);\
    document.addEventListener('dragstart', handleDragStart);\
    document.addEventListener('keydown', handleKeyDown);\
\
    return () => \{\
      document.removeEventListener('contextmenu', handleContextMenu);\
      document.removeEventListener('dragstart', handleDragStart);\
      document.removeEventListener('keydown', handleKeyDown);\
    \};\
  \}, []);\
\
  const openLightbox = (album) => \{\
    setSelectedAlbum(album);\
    setLightboxOpen(true);\
  \};\
\
  const closeLightbox = () => \{\
    setLightboxOpen(false);\
    setSelectedAlbum(null);\
  \};\
\
  // \uc0\u23450 \u32681 \u26381 \u21209 \u38917 \u30446 \u23376 \u36984 \u21934 \
  const serviceSubTabs = [\
    \{ id: 'rules', label: '\uc0\u38928 \u32004 \u38920 \u30693 ', icon: ClipboardList \},\
    \{ id: 'eyebrows', label: '\uc0\u31934 \u32251 \u38695 \u30473 ', icon: EyebrowIcon \},\
    \{ id: 'lips', label: '\uc0\u21767 \u33394 \u26657 \u27491 ', icon: LipsIcon \},\
    \{ id: 'eyeliner', label: '\uc0\u32654 \u30643 \u30524 \u32218 ', icon: Eye \},\
    \{ id: 'lashes', label: '\uc0\u30571 \u27611 \u31649 \u29702 ', icon: Clock \},\
  ];\
\
  // \uc0\u20027 \u36984 \u21934 \
  const mainTabs = [\
    \{ id: 'services', label: '\uc0\u26381 \u21209 \u38917 \u30446 ', icon: Sparkles, hasSubmenu: true \},\
    \{ id: 'pricelist', label: '\uc0\u20729 \u30446 \u34920 ', icon: DollarSign \}, \
    \{ id: 'gallery', label: '\uc0\u20316 \u21697 \u38598 ', icon: ImageIcon \},\
    \{ id: 'reviews', label: '\uc0\u22909 \u35413 \u21453 \u39243 ', icon: MessageSquare \},\
    \{ id: 'location', label: '\uc0\u20301 \u32622 \u36039 \u35338 ', icon: MapPinIcon \},\
    \{ id: 'courses', label: '\uc0\u25945 \u23416 \u35506 \u31243 ', icon: GraduationCap \},\
  ];\
\
  const handleMainTabClick = (tabId) => \{\
    setMainNav(tabId);\
    if (tabId === 'services') \{\
      setActiveTab('rules'); \
    \} else \{\
      setActiveTab(tabId);\
    \}\
    window.scrollTo(\{ top: 0, behavior: 'smooth' \});\
  \};\
\
  const renderContent = () => \{\
    switch(activeTab) \{\
      case 'home':\
        return (\
          <div className="space-y-8 animate-fadeIn">\
            <div className="bg-[#fffefb] p-8 md:p-12 shadow-sm border border-[#fee9a7]/50 text-center rounded-lg relative overflow-hidden">\
               \{/* \uc0\u35037 \u39166 \u24615 \u32972 \u26223 \u20803 \u32032  */\}\
               <div className="absolute top-0 left-0 w-full h-1.5 bg-gradient-to-r from-[#fed455] via-[#fee9a7] to-[#fed455]"></div>\
               <div className="absolute -left-10 -bottom-10 opacity-5 pointer-events-none">\
                  <Sparkles className="w-40 h-40 text-[#b08d55]" />\
               </div>\
\
               <h3 className="text-2xl md:text-3xl font-serif text-[#433422] mb-8 tracking-widest mt-4">Brand Story</h3>\
                \
               <div className="max-w-lg mx-auto space-y-6 text-[#594a42] leading-loose text-sm md:text-base tracking-wide">\
                 <p>\
                   \uc0\u32654 \u65292 \u28961 \u38656 \u24373 \u25562 \u65292 \u23427 \u26159 \u20839 \u24515 \u23527 \u38748 \u30340 \u25237 \u23556 \u12290 <br/>\
                   \uc0\u25105 \u20497 \u19981 \u36861 \u36880 \u21927 \u22057 \u30340 \u28526 \u27969 \u65292 <br/>\
                   \uc0\u21482 \u23560 \u27880 \u26044 \u24744 \u33287 \u29983 \u20465 \u20358 \u30340 \u29544 \u29305 \u27683 \u36074 \u12290 \
                 </p>\
                 <p>\
                   \uc0\u36879 \u36942 \u32048 \u33193 \u30340 \u25216 \u27861 \u65292 <br/>\
                   \uc0\u36996 \u21407 \u24744 \u26412 \u35442 \u25793 \u26377 \u30340 \u24478 \u23481 \u33287 \u20778 \u38597 \u12290 \
                 </p>\
                 <p className="font-serif text-[#b08d55] pt-4">\
                   \uc0\u22312  Yseulto.art\u65292 <br/>\
                   \uc0\u36935 \u35211 \u37027 \u20491 \u65292 \u23433 \u38748 \u32780 \u26377 \u21147 \u37327 \u30340 \u33258 \u24049 \u12290 \
                 </p>\
               </div>\
\
               <div className="flex justify-center gap-6 mt-10">\
