// \uc0\u36628 \u21161 \u20989 \u24335 \u65306 \u24478  Google Drive \u32178 \u22336 \u20013 \u25552 \u21462 \u27284 \u26696  ID\
const extractDriveId = (url) => \{\
  // \uc0\u25903 \u25588 \u22810 \u31278  Drive \u32178 \u22336 \u26684 \u24335 \
  const match = url.match(/\\/d\\/([a-zA-Z0-9_-]+)/);\
  return match ? match[1] : url; // \uc0\u22914 \u26524 \u25214 \u19981 \u21040 \u65292 \u20551 \u35373 \u36664 \u20837 \u30340 \u23601 \u26159  ID (fallback)\
\};\
